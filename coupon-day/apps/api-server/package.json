{
  "name": "@coupon-day/api-server",
  "version": "1.0.0",
  "private": true,
  "description": "CouponDay API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --skipLibCheck",
    "start": "npm run build && prisma generate && prisma db push --accept-data-loss && npm run db:seed || true && node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma": "prisma",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.0.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.0",
    "fastify": "^4.25.0",
    "fastify-plugin": "^5.1.0",
    "firebase-admin": "^13.6.0",
    "ioredis": "^5.8.2",
    "nanoid": "^5.0.0",
    "qrcode": "^1.5.0",
    "sharp": "^0.34.5",
    "zod": "^3.22.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.0",
    "@types/sharp": "^0.32.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
